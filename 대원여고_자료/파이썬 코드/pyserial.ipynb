   "source": [
    "import serial\n",
    "\n",
    "ser = serial.Serial('COM29', 9600)\n",
    "\n",
    "while True:\n",
    "    a = input()\n",
    "    \n",
    "    if a == 'q':\n",
    "        break\n",
    "        \n",
    "    ser.write(a.encode())\n",
    "    \n",
    "ser.close()"
   ]
